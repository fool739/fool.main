    <style>
      .angel {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: -1;
        pointer-events: none;
        opacity: 0.05;
        overflow: hidden;
        white-space: pre;
        font-family: monospace;
        font-size: 5px;
        line-height: 1;
        color: #ffffff;
        animation: pulse 4s ease-in-out infinite;
      }
    
      .angel pre {
        position: absolute;
        top: 50%;
        left: 90%;
        transform: translate(-50%, -50%);
      }

      @keyframes pulse { 
    0%, 100% {
      opacity: 0.03;
      text-shadow: 0 0 4px rgba(255, 255, 255, 0.1);
    }
    50% {
      opacity: 0.08;
      text-shadow: 0 0 16px rgba(255, 255, 255, 0.3);
    }
      }
